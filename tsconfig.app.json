{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": [
    "src/@types/env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "src/renderer/**/*.vue",
    "src/renderer/**/*",
    "src/@types"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ],
  "compilerOptions": {
    "typeRoots": [
      "./node_modules/@types",
      "src/@types"
    ],
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
      "@routes/*": [
        "./src/renderer/pages/*"
      ],
      "@renderer": [
        "./src/renderer/*"
      ],
      "@assets/*": [
        "./src/renderer/assets/*"
      ],
      "@components/*": [
        "./src/renderer/components/*"
      ],
      "@pages/*": [
        "./src/renderer/pages/*"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@types/*": [
        "./src/@types/*"
      ],
      "@store/*": [
        "./src/stores/*"
      ],
    },
    "types": [
      "node",
      "app"
    ]
  }
}
